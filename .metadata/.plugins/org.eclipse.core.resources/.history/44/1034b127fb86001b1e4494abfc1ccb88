package com.ibm;

public class Developer extends Employee {
	public Developer(String name, int id) {
		super(name, id);
		// TODO Auto-generated constructor stub
	}
	void analyseBug(Bug bug) {
		System.out.println("analysed");
		statusBug(bug);
		}
//	STATUS statusBug() {
//		System.out.println("status set");
//		return(STATUS.FIXED);
//	}
	void statusBug(Bug bug) {
		bug.setStatus(STATUS.FIXED);
		System.out.println("status set: "+bug.getStatus());
		
}
	void bugReport() {
			System.out.println("bug reported");
	}
	

}
